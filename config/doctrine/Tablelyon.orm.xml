    <field name="lastname" type="string" column="lastname" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="username" type="string" column="username" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="roles" type="string" column="roles" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="titre" type="text" column="titre" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
